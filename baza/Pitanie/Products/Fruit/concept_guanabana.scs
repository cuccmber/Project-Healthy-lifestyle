        <- definition;;
		=> nrel_main_idtf: [Опр.(гуанабана)] (* <- lang_ru;; *);;
		<= nrel_sc_text_translation: ...
			(*
			-> rrel_example:[Гаунабана – экзотическое дерево, которое в комнатных условиях обычно не достигает больше тридцати сантиметров в высоту. Что касается дикой природы, растение может достигать девять-десять метров, плоды при этом могут весить больше семи килограммов. В дикой природе саусеп можно встретить на территории Латинской Америки, эта же страна является исторической родиной растения. Кроме этого, можно также встретить дерево на любой территории с тропическим жарким климатом.](*<- lang_ru;;*);;
			*);;
		*);

