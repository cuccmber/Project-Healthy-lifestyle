<- rrel_key_sc_element: ... 
		(*
        <- definition;;
		=> nrel_main_idtf: [Опр.(черимойя)] (* <- lang_ru;; *);;
		<= nrel_sc_text_translation: ...
			(*
			-> rrel_example:[Черимойя — дерево рода Аннона, семейство Анноновые. Другие названия этого растения чиримола, сыча, жемчужина Анд, чирмолия и золотое перуанское яблоко. Название произошло из двух слов языка индейцев кечуа: chiri — «холодный» и moya — «семечко». Объясняется такое название выносливостью дерева к погодным условиям. Оно прекрасно растет в засушливых и прохладных местностях.](*<- lang_ru;;*);;
			*);;
		*);

