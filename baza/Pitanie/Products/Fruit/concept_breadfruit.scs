        <- statement;;
		=> nrel_main_idtf: [Утв. 1 (О хлебном дереве)] (* <- lang_ru;; *);;
		<= nrel_sc_text_translation: ...
			(*
			-> rrel_example:[Все хлебные деревья быстро растут и отличаются обилием урожая. При благоприятных условиях одно дерево приносит в год до 200 плодов. Сами плоды имеют округлую форму и весьма внушительный вес — до 15-30 кг.](*<- lang_ru;;*);;
			*);;
		*);

