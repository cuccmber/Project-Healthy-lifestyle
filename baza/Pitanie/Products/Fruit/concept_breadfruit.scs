<- rrel_key_sc_element: ... 
		(*
        <- definition;;
		=> nrel_main_idtf: [Опр.(хлебное дерево)] (* <- lang_ru;; *);;
		<= nrel_sc_text_translation: ...
			(*
			-> rrel_example:[Хлебное дерево – экзотическое растение, с которым впервые познакомились жители Океании, где оно и произрастает. Высокую ценность она представляет из-за того, что может заменить обычный хлеб. Поэтому для жителей Океании его плоды стали настоящим спасением в неурожайные годы.](*<- lang_ru;;*);;
			*);;
		*);

<=nrel_inclusion:concept_fruit;

